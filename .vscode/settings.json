{
    "pylint.enabled": true,
    "pylint.args": ["--max-line-length=120",
                    "--disable=E1101,E0611,E0401,E0633,R0801",
                    "--max-args=10",
                    "--max-locals=30",
                    "--max-attribute=15",
                    "--extension-pkg-whitelist=PyQt5,win32gui"],
    "python.testing.unittestEnabled": true,
    "python.testing.pytestEnabled": false,
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        "./tests",
        "-p",
        "test_*.py"
    ],
    "files.exclude": {
    "**/__pycache__": true,
    ".coverage": true,
    "**/Bytetrack": true,
    "**/Mmengines": true,
    "**/*.egg-info": true,
    },
}